Domain {
    Filename = "Outout3.raw"
    nproc = 1, 1, 1     // Number of processors (Npx,Npy,Npz)
    n = 200, 202, 202      // Size of local domain (Nx,Ny,Nz)
    N = 200, 202, 202      // size of the input image
    n_spheres = 1       // Number of spheres
    L = 1, 1, 1         // Length of domain (x,y,z)
    BC = 0              // Boundary condition type
    Sw = 0.15
    ReadType = "8bit"
    ReadValues = 0, 1, 2 
    WriteValues = 0, 2, 2
}

Analysis {
    tolerance = 1.0e-5
    morph_interval = 20000
    morph_delta = -0.1
    blobid_interval = 1000      // Frequency to perform blob identification
    analysis_interval = 1000    // Frequency to perform analysis
    restart_interval = 100000    // Frequency to write restart data
    visualization_interval = 1000000        // Frequency to write visualization data
    restart_file = "Restart"    // Filename to use for restart file (will append rank)
    N_threads    = 4            // Number of threads to use
    load_balance = "independent" // Load balance method to use: "none", "default", "independent"
}


MRT {
    timestepMax = 30000
    tau = 0.7
    F = 1.0e-5, 0, 0
