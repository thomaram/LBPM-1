    tauB = 0.7;  
    rhoA   = 1.0;  
    rhoB   = 1.0;  
    alpha = 1e-3;
    beta  = 0.95;
    F = 1.0e-5, 0, 0.0e-5
    Restart = false
    pBC = 0
    din = 1.0
    dout = 1.0
    timestepMax = 2500000
    interval = 1000
    tol = 1e-5;
    das = 0.1
    dbs = 0.9
